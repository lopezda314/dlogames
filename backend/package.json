    "graphql-cli": "^2.16.7",
    "graphql-yoga": "1.16.2",
    "nodemon": "^1.18.7",
    "npm-run-all": "^4.1.5",
    "prisma": "1.17.1",
    "prisma-binding": "2.1.6"
  },
  "author": "dlo"
}
